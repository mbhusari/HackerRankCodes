/* you only have to complete the function given below.  
Node is defined as  

class Node {
    int data;
    Node left;
    Node right;
}

*/

void Preorder(Node root) {
    Node node = root;
    if (node == null)
          return;
 
     System.out.print(node.data+" ");  

     Preorder(node.left);  
     Preorder(node.right); 
 }
